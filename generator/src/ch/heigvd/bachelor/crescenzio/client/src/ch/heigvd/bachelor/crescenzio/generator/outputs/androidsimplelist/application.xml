<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<application>
	<name>AndroidSimpleList</name>
	<applicationFields>
		<field type="file">
			<name>Application icon 72px</name>
		</field>
		<field type="string">
			<name>Application title</name>
		</field>
	</applicationFields>
	<mappedFields>
		<field>
			<name>Name</name>
		</field>
		<field>
			<name>Icon</name>
		</field>
		<field>
			<name>Date</name>
		</field>
		<field>
			<name>Description</name>
		</field>
	</mappedFields>
	<itemTypes>
		<itemType>
			<name>__default</name>
			<resources>
				<resource>
					<name>itemListViewFile</name>
					<extensions>
						<extension>xml</extension>
					</extensions>
					<description>list view</description>
					<value>code/itemListView.xml</value>
				</resource>
				<resource>
					<name>itemViewFile</name>
					<extensions>
						<extension>xml</extension>
					</extensions>					
					<description>itemView</description>
					<value>code/itemView.xml</value>
				</resource>
			</resources>
		</itemType>
	</itemTypes>
