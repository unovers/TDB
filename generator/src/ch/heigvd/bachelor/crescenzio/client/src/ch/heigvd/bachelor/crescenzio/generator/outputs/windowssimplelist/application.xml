<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<application>
	<name>WindowsSimpleList</name>
	<applicationFields>
		<field type="file">
			<name>applicationIcon72</name>
			<description>Application icon 72px</description>
			<extensions>
				<extension>jpg</extension>
			</extensions>			
			<value>res\drawable-hdpi\logo.png</value>
		</field>
		<field type="string">
			<name>applicationTitle</name>
			<description>Application title</description>
			<value>Application</value>
		</field>
	</applicationFields>
	<mappedFields>
		<field>
			<name>Name</name>
		</field>
		<field>
			<name>Icon</name>
		</field>
		<field>
			<name>Date</name>
		</field>
		<field>
			<name>Description</name>
		</field>
	</mappedFields>
	<itemTypes>
		<itemType>
			<name>Default</name>
			<resources>
				<resource>
					<name>RowXAMLFile</name>
					<extensions>
						<extension>xaml</extension>
					</extensions>
					<description>Row XAML file</description>
					<value>res\layout\default_row.xaml</value>
				</resource>
				<resource>
					<name>RowCSFile</name>
					<extensions>
						<extension>cs</extension>
					</extensions>					
					<description>Row CS file</description>
					<value>src\row\DefaultRow.cs</value>
				</resource>
				<resource>
					<name>PageXAMLFile</name>
					<extensions>
						<extension>java</extension>
					</extensions>					
					<description>Page XAML file</description>
					<value>src\Default.xaml</value>
				</resource>				
				<resource>
					<name>PageCSFile</name>
					<extensions>
						<extension>java</extension>
					</extensions>					
					<description>Activity src file</description>
					<value>src\Default.cs</value>
				</resource>				
			</resources>
		</itemType>
	</itemTypes>
</application>